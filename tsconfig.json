{
  "display": "Node 14",
  "exclude": [
    "_test_"
  ],
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "target": "es2020",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true, /* Ensure that casing is correct in imports. */
    "strict": true, /* Enable all strict type-checking options. */
    "skipLibCheck": true,
    "outDir": "./lib", /* Skip type checking all .d.ts files. */
    "moduleResolution": "node"
  }
}